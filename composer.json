{
  "name": "willdurand/expose-translation-bundle",
  "description": "A pretty nice way to expose your translation messages to your JavaScript.",
  "keywords": ["translation", "javascript"],
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "William Durand",
      "email": "william.durand1@gmail.com"
    }
  ],
  "require": {
    "symfony/framework-bundle": ">=2.0,<2.3-dev",
    "symfony/finder": ">=2.0,<2.3-dev",
    "symfony/console": ">=2.0,<2.3-dev"
  },
  "require-dev": {
    "symfony/yaml": ">=2.0,<2.3-dev",
    "symfony/browser-kit": ">=2.0,<2.3-dev"
  },
  "autoload": {
    "psr-0": { "Bazinga\\ExposeTranslationBundle": "" }
  },
  "target-dir": "Bazinga/ExposeTranslationBundle",
  "extra": {
    "branch-alias": {
      "dev-master": "0.2.x-dev"
    }
  },
  "minimum-stability": "dev"
}
